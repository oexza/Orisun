{
    "name": "orisun/php-client",
    "description": "PHP client library for Orisun Event Store",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Orisun Team",
            "email": "team@orisun.dev"
        }
    ],
    "require": {
        "php": ">=8.1",
        "grpc/grpc": "^1.57",
        "google/protobuf": "^3.24",
        "psr/log": "^3.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^12.3",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Orisun\\Client\\": "src/",
            "Eventstore\\": "generated/Eventstore/",
            "GPBMetadata\\": "generated/GPBMetadata/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orisun\\Client\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src tests --level=8",
        "cs-check": "phpcs src tests --standard=PSR12",
        "cs-fix": "phpcbf src tests --standard=PSR12"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
